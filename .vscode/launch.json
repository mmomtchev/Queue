{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch mocha",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/node_modules/mocha/lib/cli/cli.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Launch test",
			"runtimeExecutable": "ts-node",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/test/stress.ts"
		}
	]
}